<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cwfkm.dao.SysReplyDao">
	<update id="updateObject" parameterType="com.cwfkm.pojo.Reply">
		insert into sys_reply
		values(
		#{id},
		#{postyId},
		#{userId},
		#{context},
		#{createdTime},
		#{boardId},
		#{valid}
		)
	</update>
</mapper>




